a = list(map(int,input("Set A: ").split()))
A = set(a)
b = list(map(int,input("Set B: ").split()))
B = set(b)

# Write your code here to perform different operations
unionresult=A.union(B)
print(f"Union:  {unionresult}")
intersectionresult = A&B
print(f"Intersection:  {intersectionresult}")
diffrenceresult = A-B
print(f"Difference:  {diffrenceresult}")
